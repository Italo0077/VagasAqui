
public class Agendar2 extends Assinatura{
    
    public String escolha;
    
    public void agendar(){
        
        System.out.println("Deseja agendar uma vaga? s/n ");
		
	switch(this.escolha){
        
        case"s":System.out.println("Verificando sua assinatura. . .");
        if Assinatura=true{
            System.out.println("Vaga agendada com sucesso!");
            
        }
        else Assinatura= false{
            System.out.println("Você não é um assinante");
            
        }
        case"n":System.out.println("Voltando. . .");
        break;
        
        
        }
    }
    
}
